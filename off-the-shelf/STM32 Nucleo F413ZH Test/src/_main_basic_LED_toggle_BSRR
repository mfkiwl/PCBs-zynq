//GPIOx_BSSR - BPIO set and reset

#include "stm32f4xx_hal.h"

void delayMs(int delay);

int main(void)
{
  RCC->AHB1ENR |= 2;
  GPIOB->MODER |= 0x4000;
  while(1)
  {
    GPIOB->BSRR = 0x80;
    delayMs(1);
    GPIOB->BSRR = 0x800000;
    delayMs(1000);
  }
}

void delayMs(int delay)
{
    volatile int i;
    for ( ; delay > 0; delay--)
    {
      for (i = 0; i < 2000; i++){;}
    }
}
